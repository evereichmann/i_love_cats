<h1>Cat Show Page</h1>
<h2><%= @cat.name %></h2>
<p><%= @cat.breed %></p>
<p><%= @cat.age %></p>
<img src="<%= @cat.img_url %>"width="300" height="200" alt="<%= @cat.name%>">

<%= button_to "DELETE", cat_path(@cat), method: :delete%>
<%= button_to "EDIT", { :action => "edit" }, :method => :get %>
<%= button_to "INDEX", {:action => "index"}, :method => :get %>
